{
  "short_name": "WebTxtSum",
  "name": "Web Article Summarizer",
  "description":"A web article summarizing extension",
  "icons": {
    "16":"logo192.png",
    "48":"logo192.png",
    "128":"logo192.png"
  },
  "version":"1.0.0",
  "manifest_version": 3,
  "background":{
    "scripts":["background.js"]
  },
  "action":{
    "default_popup":"index.html",
    "default_title":"Open the summarizer"
  },
  "permissions":[
    "scripting",
    "tabs",
    "activeTab"
  ]
}
